class Vec2 {
    int32 x;
    int32 y;

    Vec2(&this, int32 x, int32 y) {
        this.x = x;
        this.y = y;
    }

    Vec2 add(&this, Vec2 other) {
        return Vec2(this.x + other.x, this.y + other.y);
    }
}

v1 = Vec2(3, 4);
v2 = Vec2(1, 2);
v3 = v1.add(v2);
print("v3: (" + v3.x + ", " + v3.y + ")");