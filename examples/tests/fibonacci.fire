// Recursive Fibonacci test using functions

int fib(int n) {
    if (n <= 1) {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

int n = int(input("How many Fibonacci numbers do you want to generate? "));
if (n <= 0) {
    print("Please enter a positive integer.");
} else {
    int i = 0;
    while (i < n) {
        print(fib(i));
        i = i + 1;
    }
}