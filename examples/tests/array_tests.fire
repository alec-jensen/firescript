// Fixed-size Array Test Suite

// Test 1: Array declaration and initialization
print("Test 1: Array declaration and initialization");
int32[] numbers = [10, 20, 30, 40, 50];
print(numbers);

// Test 2: Array indexing (access)
print("\nTest 2: Array indexing");
print(numbers[0]);  // First element
print(numbers[2]);  // Middle element
print(numbers[4]);  // Last element

// Test 3: Array length property
print("\nTest 3: Array length property");
print(numbers.length()); // 5

// Test 4: Iterate over array
print("\nTest 4: Iterate over array");
uint8 i = 0;
while (i < numbers.length()) {
	print(numbers[i]);
	i = i + 1;
}

// Test 5: Array of strings
print("\nTest 5: Array of strings");
string[] names = ["Alice", "Bob", "Charlie"];
print(names);
print(names.length()); // 3
print(names[1]);       // Bob

// End of tests
print("\nAll array tests completed!");