import @firescript/std.io.{println};
import @firescript/std.math.{max, min, abs, clamp}

// Test with int32
int32 a = 10;
int32 b = 20;
int32 maxVal = max(a, b);
int32 minVal = min(a, b);

println(maxVal);
println(minVal);

// Test with float64
float64 x = 3.14;
float64 y = 2.71;
float64 maxFloat = max(x, y);

println(maxFloat);

// Test abs with int64
int64 negNum = -42i64;
int64 absVal = abs(negNum, 0i64);

println(absVal);

// Test clamp
int32 clamped = clamp(100, 0, 50);
println(clamped);