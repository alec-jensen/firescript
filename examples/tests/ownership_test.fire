// Test basic ownership semantics

directive enable_drops;

import @firescript/std.io.{println};

// Test 1: Basic class as Owned type
class Point {
    int32 x;
    int32 y;
}

// Create an owned value using field-based constructor
Point p1 = Point(10i32, 20i32);

// Move ownership
Point p2 = p1;  // p1 is now invalid

// This should work (p2 is valid)
println(p2.x);

// Test 2: Copyable types
int32 a = 5i32;
int32 b = a;  // Copy, not move
println(a);  // Should work - a is still valid
println(b);
