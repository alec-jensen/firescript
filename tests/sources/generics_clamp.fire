// Test generic clamp function

import @firescript/std.io.{println};

T clamp<T: int32 | int64 | float32 | float64>(T value, T min, T max) {
    if (value < min) {
        return min;
    }
    if (value > max) {
        return max;
    }
    return value;
}

// Test with int32
int32 val1 = clamp(5, 10, 20);
println(val1);  // 10 (clamped to min)

int32 val2 = clamp(15, 10, 20);
println(val2);  // 15 (within range)

int32 val3 = clamp(25, 10, 20);
println(val3);  // 20 (clamped to max)

// Test with float64
float64 val4 = clamp(0.5, 1.0, 5.0);
println(val4);  // 1.0 (clamped to min)

float64 val5 = clamp(3.0, 1.0, 5.0);
println(val5);  // 3.0 (within range)

float64 val6 = clamp(10.0, 1.0, 5.0);
println(val6);  // 5.0 (clamped to max)
